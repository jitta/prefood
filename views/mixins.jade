mixin feedback(type)
  -
    var headerDict = {
      good: "What went well?",
      bad: "What didn't go so well?",
      improve: "What can do better next time?",
      foodName: "Food name"
    }
    var header = headerDict[type] 
    function shuffle(o) {
      var j, x, i;
      for (i = o.length; i; i -= 1) {
        j = Math.floor(Math.random() * i);
        x = o[i-1];
        o[i-1] = o[j];
        o[j] = x;
      }

      return o;
    }
  .row
    .col.s12.m8
      ul.collection.with-header
        li.collection-header
          h5=header
        each val in shuffle(food.ratings)
          if val.feedback[type] && val.feedback[type].length > 2
            li.collection-item= val.feedback[type]
